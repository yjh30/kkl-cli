{
  "name": "kkl-cli",
  "version": "1.0.3",
  "description": "基于vue移动端组件的脚手架工具",
  "bin": {
    "kkl": "index.js"
  },
  "main": "index.js",
  "scripts": {
    "changelog": "node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjh30/kkl-cli.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yjh30/kkl-cli/issues"
  },
  "homepage": "https://github.com/yjh30/kkl-cli#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.17.1",
    "handlebars": "^4.0.11",
    "inquirer": "^6.1.0",
    "log-symbols": "^2.2.0",
    "ora": "^3.0.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.4",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  }
}
